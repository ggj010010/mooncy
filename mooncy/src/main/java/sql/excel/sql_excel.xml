<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="excel">
 <resultMap type="com.spring.mooncy.dto.ExcelDTO" id="ExcelDTO">

    	<result property="cust_id" column="cust_id"/>

    	<result property="cust_name" column="cust_name"/>
    	
    	<result property="cust_age" column="cust_age"/>
    	
    	<result property="cust_email" column="cust_email"/>

 </resultMap>
<insert id="insertExcel" parameterType="java.util.List">

INSERT all
<foreach collection="list" item="item" separator=" ">
INTO exceltest
(
	  cust_id
	, cust_name
	, cust_age
	, cust_email
)
VALUES
(
	  #{item.custId}
	, #{item.custName}
	, #{item.custAge}
	, #{item.custEmail}
)
</foreach>
select * from dual
</insert>
</mapper>

